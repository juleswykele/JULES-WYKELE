(function(){'use strict';let lazyloader;"loading"===document.readyState?document.addEventListener("DOMContentLoaded",()=>init()):init();function init(){if(!window.jQuery){lazyloader=new IntersectionObserver((els,observer)=>{els.forEach((e)=>{if(e.isIntersecting){e.target.onload=()=>{e.target.classList.remove('lazy-hidden');e.target.classList.add('lazy-loaded');};if(e.target.dataset.src){e.target.src=e.target.dataset.src;e.target.removeAttribute('data-src');}
if(e.target.dataset.srcset){e.target.srcset=e.target.dataset.srcset;e.target.removeAttribute('data-srcset');}
if(e.target.dataset.lazySrc){if(e.target.tagName=='DIV'){e.target.style.backgroundImage='url('+e.target.dataset.lazySrc+')';e.target.classList.add('lazy-loaded');}else if(e.target.classList.contains('sproutvideo-player')){console.log('SproutVideo Iframe detected');}else{e.target.src=e.target.dataset.lazySrc;}
if(!e.target.classList.contains('sproutvideo-player'))e.target.removeAttribute('data-lazy-src');}
observer.unobserve(e.target);}});},{root:null,threshold:0.5});}
window.addEventListener('vismeLazyLoadNow',()=>{lazyLoadNow();});window.dispatchEvent(new CustomEvent('vismeLazyLoadNow'));};function lazyLoadNow(){if(window.jQuery){jQuery(window).lazyLoadXT();}else{document.querySelectorAll('[data-src]').forEach((el)=>lazyloader.observe(el));document.querySelectorAll('[data-srcset]').forEach((el)=>lazyloader.observe(el));document.querySelectorAll('[data-lazy-src]').forEach((el)=>lazyloader.observe(el));}};})();